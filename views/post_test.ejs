<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body>
    <button onclick="clickBtn()">点我</button>
</body>
<script src="js/jquery.min.js"></script>
<script>
    $(document).ready(function () {
        
    });
    function clickBtn() {
        $.ajax({
            url: '/json',
            type:'POST',
            data:{
                str: 'aaa',
                arr: ['a', 'b']
            },
            contentType: "application/json; charset=utf8",
            dataType: 'json',
            success: function (e) {
                console.log(e);
            }
        });
        $.ajax({
            url: '/urlencoded',
            type:'POST',
            data:{
                str: 'aaa',
                arr: ['a', 'b']
            },
            contentType: "application/x-www-urlencoded; charset=utf8",
            dataType: 'json',
            success: function (e) {
                console.log(e);
            }
        });
        $.ajax({
            url: '/formurlencoded',
            type:'POST',
            data:{
                str: 'aaa',
                arr: ['a','b']
            },
            contentType: "application/x-www-form-urlencoded; charset=utf8",
            dataType: 'json',
            success: function (e) {
                console.log(e);
            }
        });
        $.ajax({
            url: '/multipart',
            type:'POST',
            data:{
                str: 'aaa',
                arr: ['a', 'b']
            },
            contentType: "multipart/form-data; charset=utf8",
            dataType: 'json',
            success: function (e) {
                console.log(e);
            }
        });
        $.ajax({
            url: '/html',
            type:'POST',
            data:{
                str: 'aaa',
                arr: ['a', 'b']
            },
            contentType: "text/html; charset=utf8",
            dataType: 'json',
            success: function (e) {
                console.log(e);
            }
        });
        $.ajax({
            url: '/default',
            type:'POST',
            data:{
                str: 'aaa',
                arr: ['a', 'b']
            },
            dataType: 'json',
            success: function (e) {
                console.log(e);
            }
        });
    }
</script>
</html>